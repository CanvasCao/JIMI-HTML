{
  "manifest_version": 2,
  "name": "jimiSDK",
  "version": "1.0",
  "description": "jimiSDKDescription",
  "icons": {
    "16": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png"
  },
  "content_scripts":[
    {
      "matches":["*://item.taobao.com/*"],
      "js":[
        "js/jquery-1.8.3.min.js",
        "js/neverClick.js"
      ],
      "css":["css/jimiSDK.css"]
    }
  ],
  "permissions": [ "tabs", "contextMenus","http://*/*", "https://*/*"]
 // "content_security_policy": "script-src 'self' https://webapi.amap.com/ https://js.webapi.amap.com/ https://restapi.amap.com/ 'unsafe-eval'; object-src 'self'"
}